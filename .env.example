# GitHub Personal Access Token (PAT)
# Required to avoid rate limits. You can provide multiple tokens (PAT_1, PAT_2, etc.)
PAT_1=

# Optional: Cache duration in seconds (default is 86400 / 24 hours)
CACHE_SECONDS=86400

# Optional: Comma-separated list of GitHub usernames that are allowed to access your instance
WHITELIST=

# Optional: Comma-separated list of GitHub Gist IDs that are allowed
GIST_WHITELIST=

# Optional: Comma-separated list of repositories that will be excluded
EXCLUDE_REPO=

# Optional: Enable fetching all starred repositories for accurate star counts (true/false)
FETCH_MULTI_PAGE_STARS=false
